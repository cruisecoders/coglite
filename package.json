{
  "name": "angular2-quickstart",
  "version": "1.0.0",
  "description": "Angular 2 QuickStart",
  "main": "server.js",
  "scripts": {
    "tsc": "tsc",
    "tsc:watch": "tsc -w",
    "start": "npm run tsc && concurrently \"npm run tsc:watch\"",
    "start_prod": "npm run tsc && concurrently \"npm run tsc:watch\" \"node server.js --NODE_ENV production\" ",
    "rimraf": "rimraf",
    "dev": "node server.js",
    "build": "npm run rimraf app/dist && webpack --config app/config/webpack.prod.js --progress --profile --bail"
  },
  "keywords": [
    "angular2",
    "quickstart"
  ],
